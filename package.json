{
  "name": "orange-hrm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run delete:report && npm run create:reportfolder",
    "cy:chrome": "npx cypress run --browser=chrome",
    "test": "npm run cy:chrome || npm run posttest",
    "posttest": "npm run mochawesome:merge && npm run generate:htmlReport",
    "delete:report": "node cypress/support/node_fs/removeDir.js",
    "create:reportfolder": "node cypress/support/node_fs/createDir.js",
    "mochawesome:merge": "mochawesome-merge cypress/results/*.json > cypress/results/mochawesome-report/report.json",
    "generate:htmlReport": "marge cypress/results/mochawesome-report/report.json -f report -o cypress/results/mochawesome-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.3.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
